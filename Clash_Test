rename_node=!!script:function rename(node) {
    const geoinfo = JSON.parse(geoip(node.Hostname));
    if (geoinfo.country && geoinfo.country_code) {
        const countryName = geoinfo.country;
        const countryCode = geoinfo.country_code;
        const countryFlag = geoinfo.emoji || "ğŸ´â€â˜ ï¸";

        return `${countryFlag} ${countryName} | ${countryCode} | @MxlShare`;
    }

    return `ğŸ´â€â˜ ï¸ æœªçŸ¥ | Unknown | @MxlShare`;
}